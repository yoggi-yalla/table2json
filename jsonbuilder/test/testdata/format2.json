{
  "functions": [
    "def f2(x): return 1+4/x",
    "def f3(x,r,df): x['foo'] = 'bar'; return x",
    "curve_translation = {'USD_OIS':'USD_SOFR','EUR_OIS':'EUR_ESTR'}"
  ],
  "df_transforms": [
    "df['discount_factor']*2",
    "translate(df['name'], curve_translation)"
  ],
  "mapping": {
    "type": "array",
    "children": [
      {
        "type": "object",
        "group_by": "name",
        "children": [
          {
            "name": "action",
            "value": "ADD_IR"
          },
          {
            "name": "currency",
            "column": "name"
          },
          {
            "type": "array",
            "name": "ir_curves",
            "children": [
              {
                "type": "object",
                "transmute": "f3(x,r,df)",
                "children": [
                  {
                    "type": "array",
                    "name": "points",
                    "children": [
                      {
                        "type": "object",
                        "iterate": true,
                        "children": [
                          {
                            "column": "date",
                            "name": "date"
                          },
                          {
                            "column": "discount_factor",
                            "transmute": "x*2",
                            "name": "df"
                          }
                        ]
                      }
                    ]
                  },
                  {
                    "name": "type",
                    "value": "BASE_CURVE"
                  },
                  {
                    "name": "identifier",
                    "column": "name"
                  }
                ]
              }
            ]
          }
        ]
      }
    ]
  }
}